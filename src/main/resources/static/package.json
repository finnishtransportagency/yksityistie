{
  "name": "drreport",
  "version": "1.0.1",
  "description": "RaportointityÃ¶kalu DR2 OTH-aineistoille",
  "main": "index.js",
  "scripts": {
    "html": "html-minifier --collapse-whitespace ./src/index.html -o ./index.html",
    "css": "stylus --include ./node_modules --include-css --compress ./src/css/main.styl -o ./styles/style.css",
    "js": "browserify ./src/js/main.js | uglifyjs > ./js/bundle.js",
    "js2": "browserify ./src/js/main.js > ./js/bundle.js",
    "js3": "watchify ./src/js/main.js --outfile ./js/bundle.js -dv",
    "fonts": "fontify",
    "build": "npm run html & npm run css & npm run js3 & npm run fonts",
    "watch-html": "nodemon --ext html --watch ./src/ --exec npm run html",
    "watch-css": "nodemon --ext styl --watch ./src/css --exec npm run css",
    "watch-js": "nodemon --ext js --watch ./src/js --exec npm run js",
    "watch-js2": "nodemon --ext js --watch ./src/js --exec npm run js2",
    "watch": "npm-run-all --parallel watch-html watch-css watch-js",
    "watch2": "npm-run-all --parallel watch-html watch-css watch-js2",
    "server": "start npm run watch && start live-server --port=3000 ."
  },
  "author": "Mika Lehtonen / Dimenteq Oy",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "bootstrap-switch": "^3.3.2",
    "c3": "^0.4.11",
    "d3": "^3.5.17",
    "eonasdan-bootstrap-datetimepicker-custom": "^4.17.39",
    "font-awesome": "^4.6.3",
    "free-jqgrid": "4.13.3",
    "jquery": "^2.2.4",
    "jquery-ui": "1.10.5",
    "noty": "^2.3.8",
    "openlayers": "^4.0.1",
    "select2": "4.0.3",
    "select2-bootstrap-css": "^1.4.6",
    "sockjs-client": "^1.1.1",
    "stompjs": "^2.3.3",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "fontify": "0.0.2",
    "html-minifier": "^2.1.3",
    "live-server": "^1.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "stylus": "^0.54.5",
    "uglify-js": "^2.6.2"
  }
}
